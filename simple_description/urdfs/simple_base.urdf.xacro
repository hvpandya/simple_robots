<?xml version="1.0"?>
<robot name="simple_base" xmlns:xacro="http://wiki.ros.org/xacro">
  <xacro:property name="M_PI" value="3.14159265"/>
  <!-- links -->
  <link name="base_footprint"/>
  <link name="base_link">
    <visual>
      <geometry>
        <cylinder length="0.1" radius="0.2"/>
      </geometry>
      <material name="DarkGray">
        <color rgba="0.175 0.175 0.175 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.1" radius="0.2"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0.0 0"/>
        <mass value="20"/>
      <inertia ixx="0.21667" ixy="0" ixz="0" iyy="0.21667" iyz="0" izz="0.4"/>
    </inertial>
  </link>
  <link name="right_wheel_link">
    <visual>
      <geometry>
        <cylinder length="0.025" radius="0.1"/>
      </geometry>
      <material name="Orange">
        <color rgba="1.0 0.5088 0.0468 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.025" radius="0.1"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
        <mass value="1"/>
      <inertia ixx="0.00255" ixy="0" ixz="0" iyy="0.00255" iyz="0" izz="0.005"/>
    </inertial>
  </link>
  <link name="left_wheel_link">
    <visual>
      <geometry>
        <cylinder length="0.025" radius="0.1"/>
      </geometry>
      <material name="Orange">
        <color rgba="1.0 0.5088 0.0468 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.025" radius="0.1"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
        <mass value="1"/>
      <inertia ixx="0.00255" ixy="0" ixz="0" iyy="0.00255" iyz="0" izz="0.005"/>
    </inertial>
  </link>
  <link name="caster_wheel_link">
    <visual>
      <geometry>
        <sphere radius="0.05"/>
      </geometry>
      <material name="DarkGray">
        <color rgba="0.175 0.175 0.175 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <sphere radius="0.05"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
        <mass value="1"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>
  </link>
  <link name="laser_link">
    <visual>
      <geometry>
        <cylinder length="0.05" radius="0.05"/>
      </geometry>
      <material name="Blue">
        <color rgba="0 0 1 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.05" radius="0.05"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
        <mass value="1"/>
      <inertia ixx="0.00083" ixy="0" ixz="0" iyy="0.00083" iyz="0" izz="0.00125"/>
    </inertial>
  </link>
  <link name="camera_link">
    <visual>
      <geometry>
        <box size="0.02 0.1 0.02"/>
      </geometry>
      <material name="Blue">
        <color rgba="0 0 1 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="0.02 0.1 0.02"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
        <mass value="0.2"/>
      <inertia ixx="0.00017" ixy="0" ixz="0" iyy="0.000013" iyz="0" izz="0.00017"/>
    </inertial>
  </link>
  <link name="imu_link"/>

  <!-- joints -->
  <joint name="base_footprint_joint" type="fixed">
    <parent link="base_footprint"/>
    <child link="base_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>
  <joint name="right_wheel_joint" type="continuous">
    <origin rpy="-${M_PI/2} 0 0" xyz="0 -0.2125 0"/>
    <parent link="base_link"/>
    <child link="right_wheel_link"/>
    <axis xyz="0 0 1"/>
  </joint>
  <joint name="left_wheel_joint" type="continuous">
    <origin rpy="-${M_PI/2} 0 0" xyz="0 0.2125 0"/>
    <parent link="base_link"/>
    <child link="left_wheel_link"/>
    <axis xyz="0 0 1"/>
  </joint>
  <joint name="caster_wheel_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0.1 0 -0.05"/>
    <parent link="base_link"/>
    <child link="caster_wheel_link"/>
  </joint>
  <joint name="laser_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0.1 0 0.075"/>
    <parent link="base_link"/>
    <child link="laser_link"/>
  </joint>
  <joint name="camera_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0.2 0 0.025"/>
    <parent link="base_link"/>
    <child link="camera_link"/>
  </joint>
  <joint name="imu_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0.05"/>
    <parent link="base_link"/>
    <child link="imu_link"/>
  </joint>

</robot>
